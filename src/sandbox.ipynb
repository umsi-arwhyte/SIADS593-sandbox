    "\n",
    "\n",
    "def sum_of_squares(numbers):\n",
    "    return sum(x**2 for x in numbers)\n",
    "\n",
    "\n",
    "result = sum_of_squares(nums)\n",
    "print(f\"The sum of squares of {nums} is {result}\")\n"
